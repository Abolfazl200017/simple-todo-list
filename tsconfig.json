{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      // "@components/*": ["components/*"],
      // "@layouts/*": ["layouts/*"],
      // "@pages/*": ["pages/*"],
      // "@utils/*": ["utils/*"],
      // "@assets/*": ["assets/*"],
      // "@config/*": ["config/*"],
      // "@navigation/*": ["navigation/*"],
      // // "@redux/*": ["redux/*"],
      // "@services/*": ["services/*"],
      // "@styles/*": ["styles/*"],
      "components/*": ["components/*"],
      "layouts/*": ["layouts/*"],
      "pages/*": ["pages/*"],
      "utils/*": ["utils/*"],
      "assets/*": ["assets/*"],
      "config/*": ["config/*"],
      "navigation/*": ["navigation/*"],
      // "redux/*": ["redux/*"],
      "services/*": ["services/*"],
      "styles/*": ["styles/*"],
      "contexts/*": ["contexts/*"],
    },
    "jsx": "react-jsx",
    "module": "esnext",
    "target": "es6",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "allowImportingTsExtensions": true,  // Allow importing .ts/.tsx files with extensions
    "noEmit": true  // Enable noEmit
  },
  "include": [
    "src"
  ]
}
